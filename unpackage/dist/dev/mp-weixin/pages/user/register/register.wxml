<view><image class="user-head-image" src="../../../static/user_bg.png" mode="aspectFill"></image><input class="input-view" type="text" placeholder="请输入账号" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/><input class="input-view" type="text" password="true" placeholder="请输入密码" maxlength="16" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><input class="input-view" type="text" password="true" placeholder="确认密码" maxlength="16" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/><view class="view-register"><text class="text-none">已有账号?</text><text data-event-opts="{{[['tap',[['jumpUrl']]]]}}" class="text-register" bindtap="__e">立即登录</text></view><button class="btn-register" form-type="submit" data-event-opts="{{[['tap',[['register']]]]}}" bindtap="__e">注册</button></view>