<view class="tabs"><scroll-view class="tab-bar" scroll-x="{{true}}" show-scrollbar="{{false}}" scroll-into-view="{{scrollInto}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="tab-item" id="{{'tab_'+tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['mSelectedTab',['$event']]]]]}}" bindtap="__e"><text class="{{['tab-item-title',mSelectedIndex==index?'title-active':'']}}">{{tab.name}}</text></view></block></scroll-view><swiper class="swiper" current="{{mSelectedIndex}}" data-event-opts="{{[['change',[['mTabPageSwipe',['$event']]]]]}}" bindchange="__e"><block wx:for="{{newsList}}" wx:for-item="tab" wx:for-index="index1" wx:key="index1"><swiper-item class="swiper-item"><scroll-view class="swipe-list-item" scroll-y enableBackToTop="true" loadmoreoffset="15" data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block wx:for="{{tab.data}}" wx:for-item="item" wx:for-index="index2" wx:key="id"><view data-event-opts="{{[['tap',[['jumpUrl',['$0'],[[['newsList','',index1],['data','id',item.id,'link']]]]]]]}}" bindtap="__e"><tabitem vue-id="{{'440b603c-1-'+index1+'-'+index2}}" data="{{item}}" data-event-opts="{{[['^clickCollect',[['updateCollect',['$0'],[[['newsList','',index1],['data','id',item.id]]]]]]]}}" bind:clickCollect="__e" bind:__l="__l"></tabitem></view></block><block wx:if="{{tab.isLoading||tab.data.length>4}}"><view class="loading-more"><text class="loading-more-text">{{tab.loadingText}}</text></view></block></scroll-view></swiper-item></block></swiper></view>